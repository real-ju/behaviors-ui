{
  "pages": [
    // pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    // 按钮
    {
      "path": "pages/button/basic",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 不同状态按钮
    {
      "path": "pages/button/statusSwitch",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 图标
    {
      "path": "pages/icon/basic",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 视图加载中
    {
      "path": "pages/viewLoading/basic",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 页面
    {
      "path": "pages/appPage/basic",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 沉浸式导航栏页面
    {
      "path": "pages/appPage/immersiveNav",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 下拉刷新页面
    {
      "path": "pages/appPage/refresh",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 多选器
    {
      "path": "pages/checkbox/basic",
      "style": {
        "navigationStyle": "custom"
      }
