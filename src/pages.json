    {
      "path": "pages/selectView/cascade",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 内嵌选择器-时间选择
    {
      "path": "pages/pickerView/time",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 内嵌选择器-地区选择
    {
      "path": "pages/pickerView/region",
      "style": {
        "navigationStyle": "custom"
      }
    }
