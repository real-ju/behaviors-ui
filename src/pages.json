    {
      "path": "pages/popup/basic",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 自定义位置弹出层
    {
      "path": "pages/popup/custom",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 数据操作器
    {
      "path": "pages/dataPopup/basic",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 数据操作器-列表数据筛选
    {
      "path": "pages/dataPopup/filter",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 表单-登录
    {
      "path": "pages/form/login",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 手势视图
    {
      "path": "pages/gestureView/basic",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 图片
    {
      "path": "pages/image/basic",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 输入框
    {
      "path": "pages/input/basic",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 数字输入框
    {
      "path": "pages/input/number",
      "style": {
        "navigationStyle": "custom"
      }
    },
    // 内嵌选择器-单选
    {
      "path": "pages/selectView/single",
      "style": {
        "navigationStyle": "custom"
      }
